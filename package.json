{
  "name": "syllable",
  "version": "2.0.0",
  "description": "Count syllables in English words",
  "license": "MIT",
  "keywords": [
    "detect",
    "syllables",
    "natural",
    "language",
    "cli",
    "bin"
  ],
  "repository": "https://github.com/wooorm/syllable",
  "bugs": "https://github.com/wooorm/syllable/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "problematic.json",
    "index.js",
    "cli.js"
  ],
  "bin": "cli.js",
  "dependencies": {
    "has": "^1.0.1",
    "normalize-strings": "^1.1.0",
    "pluralize": "^3.0.0",
    "trim": "0.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "esmangle": "^1.0.0",
    "execa": "^0.5.0",
    "nyc": "^10.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js -s syllable > syllable.js",
    "build-mangle": "esmangle syllable.js > syllable.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "rules": {
      "unicorn/explicit-length-check": "off",
      "guard-for-in": "off"
    },
    "ignores": [
      "syllable.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "presets": "wooorm"
  }
}
